{
  "name": "quickjs-debugger",
  "version": "2.0.0",
  "description": "A debug library for quickjs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "qjs-debugger": "./dist/repl.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
